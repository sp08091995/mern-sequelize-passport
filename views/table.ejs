<table id="tableUsers">
    <tr>
       <th>Username</th><th>First Name</th><th>Last Name</th><th>Email</th><th>Edit</th><th>Delete</th>
    </tr>
 
    <% for (var i = 0; i < users.length; /* I save the data in a variable 'quotation', I don't know how you named your variable  */  i++) { %>
     <tr>
       <td><%= users[i].username %></td>
       <td><%= users[i].firstname %></td>
       <td><%= users[i].lastname %></td>
       <td><%= users[i].email %></td>
       <td><a href="/user/edit/<%= users[i].email %>" >Edit</a></td>
       <td><a href="/user/delete/<%= users[i].email %>" >Delete</a></td>
     </tr>    
    <% } %>
 </table>